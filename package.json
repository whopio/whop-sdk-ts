{
  "name": "whop-sdk-ts",
  "version": "0.0.3-canary.6",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "resolutions": {
    "tsup": "6.5.0",
    "typescript": "4.9.3"
  },
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "packageManager": "pnpm@7.13.6"
}
