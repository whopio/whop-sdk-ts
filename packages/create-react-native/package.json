{
	"name": "@whop/create-react-native",
	"description": "Create a new React Native project for Whop",
	"version": "0.0.2",
	"repository": {
		"type": "git",
		"url": "https://github.com/whopio/whop-sdk-ts",
		"directory": "packages/create-react-native"
	},
	"keywords": ["whop", "react-native", "sdk", "embedded", "apps", "payments"],
	"bugs": "https://github.com/whopio/whop-sdk-ts/issues",
	"homepage": "https://whop.com/developers/",
	"license": "MIT",
	"files": ["dist", "template"],
	"bin": {
		"create-react-native": "./dist/index.js"
	},
	"scripts": {
		"build": "pnpm run clean && tsup",
		"clean": "rm -rf dist",
		"dev": "tsup --watch",
		"check-types": "tsc --noEmit",
		"lint:fix": "biome check --write --unsafe"
	},
	"dependencies": {
		"inquirer": "^9.0.0",
		"slugify": "^1.6.6",
		"chalk": "^5.3.0",
		"fs-extra": "^11.2.0",
		"node-fetch": "^3.3.2",
		"ora": "^6.3.1"
	},
	"devDependencies": {
		"@types/node": "latest",
		"@types/inquirer": "^9.0.0",
		"@types/fs-extra": "^11.0.0",
		"tsup": "8.5.0",
		"typescript": "latest"
	},
	"packageManager": "pnpm@9.15.9",
	"engines": {
		"node": "22.x",
		"pnpm": "9.15.9"
	},
	"publishConfig": {
		"access": "public"
	}
}
