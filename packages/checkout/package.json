{
	"name": "@whop/checkout",
	"version": "0.0.21-canary.7",
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": ["dist"],
	"scripts": {
		"build": "tsup",
		"dev": "concurrently \"pnpm run dev:tsup\" \"pnpm run dev:serve\"",
		"dev:tsup": "tsup --watch",
		"dev:serve": "serve ./dist",
		"check-types": "tsc --noEmit",
		"lint:fix": "biome check --write --unsafe",
		"release": "changeset publish"
	},
	"devDependencies": {
		"@swc/core": "^1.11.29",
		"@types/node": "latest",
		"concurrently": "9.1.2",
		"dotenv": "16.5.0",
		"serve": "^14.2.4",
		"ts-proto": "2.7.0",
		"tsup": "8.5.0",
		"typescript": "latest"
	},
	"packageManager": "pnpm@9.15.9",
	"engines": {
		"node": "22.x",
		"pnpm": "9.15.9"
	},
	"publishConfig": {
		"access": "public"
	}
}
