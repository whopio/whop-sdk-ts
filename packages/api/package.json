{
	"name": "@whop/api",
	"version": "0.0.20",
	"exports": {
		".": {
			"node": {
				"import": {
					"types": "./dist/index.node.d.mts",
					"default": "./dist/index.node.mjs"
				},
				"require": {
					"types": "./dist/index.node.d.ts",
					"default": "./dist/index.node.js"
				}
			},
			"browser": {
				"import": {
					"types": "./dist/index.browser.d.mts",
					"default": "./dist/index.browser.mjs"
				},
				"require": {
					"types": "./dist/index.browser.d.ts",
					"default": "./dist/index.browser.js"
				}
			},
			"import": {
				"types": "./dist/index.browser.d.mts",
				"default": "./dist/index.browser.mjs"
			},
			"require": {
				"types": "./dist/index.browser.d.ts",
				"default": "./dist/index.browser.js"
			}
		}
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": ["dist"],
	"scripts": {
		"build": "pnpm run proto-codegen && graphql-codegen && pnpm run lint:fix && tsup",
		"check-types": "pnpm run proto-codegen && graphql-codegen && pnpm run lint:fix && tsc --noEmit",
		"proto-codegen": "bash ./scripts/generate-protos.sh",
		"lint:fix": "biome check --write --unsafe"
	},
	"dependencies": {
		"graphql": "16.8.1",
		"graphql-request": "latest",
		"graphql-tag": "2.12.6",
		"jose": "6.0.11",
		"js-md5": "0.8.3"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "5.0.5",
		"@graphql-codegen/typescript": "4.1.5",
		"@graphql-codegen/typescript-generic-sdk": "4.0.1",
		"@graphql-codegen/typescript-operations": "4.5.1",
		"@types/node": "latest",
		"dotenv": "16.5.0",
		"ts-proto": "2.7.0",
		"tsup": "8.2.4",
		"typescript": "latest"
	},
	"packageManager": "pnpm@9.15.9",
	"engines": {
		"node": "22.x",
		"pnpm": "9.15.9"
	},
	"publishConfig": {
		"access": "public"
	}
}
