{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "@whop-sdk/core#build": {
      "dependsOn": ["spec#build"],
      "outputs": ["dist/**"],
      "cache": false,
      "inputs": ["src/**"]
    },
    "@whop-sdk/react-query#build": {
      "dependsOn": ["spec#build"],
      "outputs": ["dist/**"],
      "cache": false,
      "inputs": ["src/**"]
    },
    "@whop-sdk/svelte-query#build": {
      "dependsOn": ["spec#build"],
      "outputs": ["dist/**"],
      "cache": false,
      "inputs": ["src/**"]
    },
    "@whop-sdk/swr#build": {
      "dependsOn": ["spec#build"],
      "outputs": ["dist/**"],
      "cache": false,
      "inputs": ["src/**"]
    },
    "@whop-sdk/vue-query#build": {
      "dependsOn": ["spec#build"],
      "outputs": ["dist/**"],
      "cache": false,
      "inputs": ["src/**"]
    },
    "repo-utils#build": {
      "outputs": ["dist/**"],
      "cache": true,
      "inputs": ["src/**"]
    },
    "spec#build": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "dependsOn": ["^dev"]
    },
    "release": {
      "dependsOn": ["build"],
      "cache": false
    }
  }
}
